{{j[topology, (n1_)?Positive, (n2_)?Positive, (n3_)?NonPositive, 
     (n4_)?Positive, (n5_)?Positive] /; True :> 
   Expand[j[topology, n1, n2, n3, n4, n5]]}, 
 {j[topology, (n1_)?Positive, (n2_)?Positive, (n3_)?NonPositive, 
     (n4_)?Positive, (n5_)?Positive] /; True :> 
   Expand[j[topology, n4, n2, 0, n1, n5]/(j[topology, -1, 0, 0, 0, 0] + 
       j[topology, 0, -1, 0, 0, 0] - j[topology, 0, 0, -1, 0, 0] + 
       j[topology, 0, 0, 0, -1, 0] + 3*j[topology, 0, 0, 0, 0, -1] - 
       pp*j[topology, 0, 0, 0, 0, 0])^n3]}}
